<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/dwdqqd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base6dp"
        android:background="@drawable/xian"
        />
    <TextView
        android:id="@+id/fwaed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:text="费用报销申请列表"
        android:textSize="@dimen/base14dp"
        android:textColor="#333333"
        android:layout_below="@+id/dwdqqd"
        android:layout_marginTop="@dimen/base12dp"
        android:layout_marginLeft="@dimen/base11dp"
        />
    <ImageView
        android:id="@+id/dwdqqs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base12dp"
        android:background="@drawable/xian"
        android:layout_below="@+id/fwaed"
        />
    <com.smartwasser.yunzhishui.view.RefreshLayout
        android:id="@+id/swipe_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/dwdqqs"
        android:layout_marginTop="@dimen/base4dp"
        >
    <ListView
        android:id="@+id/lsit_reimburse_audit"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="#f2f2f2"
        android:dividerHeight="@dimen/base7dp"
        android:layout_marginTop="@dimen/base4dp"
        android:background="#ffffff"
        >
    </ListView>
    </com.smartwasser.yunzhishui.view.RefreshLayout>
    <RelativeLayout
        android:id="@+id/relative_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/dwdqqs"
        android:background="#f2f2f2"
        android:visibility="gone"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="没有要审核的数据"
            android:textSize="@dimen/base17dp"
            android:textColor="#000000"
            />
    </RelativeLayout>
</RelativeLayout>
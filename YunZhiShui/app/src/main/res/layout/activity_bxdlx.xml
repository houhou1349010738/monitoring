<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f2f2f2"
    >
    <include layout="@layout/toolbar_hitch" android:id="@+id/asas"/>
    <RelativeLayout
        android:background="#ffffff"
        android:layout_width="match_parent"
        android:layout_height="@dimen/base43dp"
        android:id="@+id/lisadhu"
        android:layout_below="@+id/asas"
        >
    <TextView
        android:id="@+id/fas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="请选择报销单类型"
        android:textSize="@dimen/base15dp"
        android:textColor="#101010"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/base10dp"
        />
    </RelativeLayout>
    <ListView
        android:id="@+id/list_contract"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/lisadhu"
        android:background="#ffffff"
        android:layout_marginTop="@dimen/base4dp"
        >
    </ListView>
</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    >
      <include layout="@layout/toolbar" android:id="@+id/taef"/>
    <LinearLayout
        android:id="@+id/layout_22"
        android:layout_width="match_parent"
        android:layout_height="@dimen/base40dp"
        android:background="#f9f1fc"
        android:layout_below="@+id/taef"
        android:orientation="horizontal"
        >
        <ImageView
            android:layout_width="@dimen/base136dp"
            android:layout_height="wrap_content"
            android:background="@drawable/xian"
            android:layout_marginTop="@dimen/base23dp"
            />
      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:textSize="@dimen/base13dp"
          android:textColor="#333333"
          android:layout_marginTop="@dimen/base15dp"
          android:text="共"
          />
        <TextView
            android:id="@+id/tv_count"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/base13dp"
            android:textColor="#333333"
            android:layout_marginTop="@dimen/base15dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/base13dp"
            android:textColor="#333333"
            android:layout_marginTop="@dimen/base15dp"
            android:text="条相关信息"
            />
        <ImageView
            android:layout_width="@dimen/base148dp"
            android:layout_height="wrap_content"
            android:background="@drawable/xian"
            android:layout_marginTop="@dimen/base23dp"
            />
    </LinearLayout>
    <TextView
        android:id="@+id/dasdww"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="审核状态"
        android:textSize="@dimen/base15dp"
        android:textColor="#101010"
        android:layout_marginTop="@dimen/base12dp"
        android:layout_below="@+id/layout_22"
        android:layout_marginLeft="@dimen/base11dp"
        />
    <ImageView
        android:id="@+id/fwdccw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dasdww"
        android:layout_marginTop="@dimen/base12dp"
        android:background="@drawable/xian"
        />
    <ListView
        android:id="@+id/list_audit_state"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/fwdccw"
        >
    </ListView>
</RelativeLayout>
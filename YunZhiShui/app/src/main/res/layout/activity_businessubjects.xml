<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    >
    <include layout="@layout/toolbar" android:id="@+id/toolbar_1"/>
    <include layout="@layout/toobar_home3" android:id="@+id/toolbar_2"/>
    <TextView
        android:id="@+id/aeffea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="选择业务科目"
        android:layout_below="@+id/toolbar_1"
        android:layout_marginTop="@dimen/base12dp"
        android:layout_marginLeft="@dimen/base12dp"
        />
    <ImageView
        android:id="@+id/adewqd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/aeffea"
        android:layout_marginTop="@dimen/base12dp"
        android:background="@drawable/xian"
        />
    <ListView
        android:id="@+id/list_business"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f9f1fc"
        android:layout_below="@+id/adewqd"
        android:layout_marginTop="@dimen/base5dp"
        >
    </ListView>
</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#ffffff"
    android:layout_height="match_parent">
     <include layout="@layout/toolbar_hitch" android:id="@+id/sfaf"/>
     <TextView
         android:id="@+id/sdasas"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:text="运输任务管理列表"
         android:layout_marginLeft="@dimen/base11dp"
         android:textSize="@dimen/base15dp"
         android:textColor="#010101"
         android:layout_marginTop="@dimen/base12dp"
         android:layout_below="@+id/sfaf"
         />
    <ImageView
        android:id="@+id/asda"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sdasas"
        android:layout_marginTop="@dimen/base12dp"
        android:background="@drawable/xian2"
        />
    <ListView
        android:id="@+id/list_transporta"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/asda"
        >
    </ListView>
</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    >
    <include layout="@layout/toolbar" android:id="@+id/dadezx"/>
    <TextView
        android:id="@+id/efwcew"
        android:layout_below="@+id/dadezx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="基本信息"
        android:layout_marginLeft="@dimen/base11dp"
        android:layout_marginTop="@dimen/base10dp"
        android:textColor="#333333"
        android:textSize="@dimen/base13dp"
        />
    <TextView
        android:id="@+id/tv_addreimbures_dh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base10dp"
        android:layout_below="@+id/dadezx"
        android:textColor="#333333"
        android:textSize="@dimen/base13dp"
        android:layout_marginRight="@dimen/base11dp"
        android:layout_alignParentRight="true"
        />
    <ImageView
        android:id="@+id/daca"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base10dp"
        android:layout_below="@+id/efwcew"
        android:background="@drawable/xian"
        />
    <ScrollView
        android:layout_below="@+id/daca"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/efoamfs"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/xaexa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="单位名称:"
                android:textSize="@dimen/base11dp"
                android:textColor="#555555"
                android:layout_marginTop="@dimen/base13dp"
                android:layout_marginLeft="@dimen/base38dp"
                />
            <EditText
                android:id="@+id/ed_addreimbures_xmmc"
                android:layout_width="@dimen/base220dp"
                android:layout_height="@dimen/base25dp"
                android:layout_toRightOf="@+id/xaexa"
                style="@style/edit"
                android:layout_marginLeft="@dimen/base5dp"
                android:layout_marginTop="@dimen/base8dp"
                android:hint="请选择"
                android:editable="false"
                />
            <ImageButton
                android:id="@+id/image_addreimburse_xmmc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/xiala2"
                android:layout_toRightOf="@+id/xaexa"
                android:layout_marginTop="@dimen/base18dp"
                android:layout_marginLeft="@dimen/base200dp"
                />
            <TextView
                android:id="@+id/adade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="业务类型:"
                android:textSize="@dimen/base11dp"
                android:textColor="#555555"
                android:layout_marginTop="@dimen/base17dp"
                android:layout_marginLeft="@dimen/base38dp"
                android:layout_below="@+id/xaexa"
                />
            <EditText
                android:id="@+id/ed_addreimbures_ywlx"
                android:layout_width="@dimen/base220dp"
                android:layout_height="@dimen/base25dp"
                android:layout_toRightOf="@+id/adade"
                android:layout_below="@+id/xaexa"
                style="@style/edit"
                android:layout_marginLeft="@dimen/base5dp"
                android:layout_marginTop="@dimen/base12dp"
                android:hint="请选择"
                android:editable="false"
                />
            <ImageButton
                android:id="@+id/image_addreimburse_ywlx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/xiala2"
                android:layout_marginTop="@dimen/base23dp"
                android:layout_marginLeft="@dimen/base200dp"
                android:layout_toRightOf="@+id/adade"
                android:layout_below="@+id/xaexa"
                />
            <TextView
                android:id="@+id/dqasdaa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="收款单位:"
                android:textSize="@dimen/base11dp"
                android:textColor="#555555"
                android:layout_marginTop="@dimen/base17dp"
                android:layout_marginLeft="@dimen/base38dp"
                android:layout_below="@+id/adade"
                />
            <EditText
                android:id="@+id/ed_addreimbures_skdw"
                android:layout_width="@dimen/base220dp"
                android:layout_height="@dimen/base25dp"
                android:layout_toRightOf="@+id/dqasdaa"
                android:layout_below="@+id/adade"
                style="@style/edit"
                android:layout_marginLeft="@dimen/base5dp"
                android:layout_marginTop="@dimen/base12dp"
                />
            <ImageView
                android:id="@+id/fwaedxs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/base10dp"
                android:background="@drawable/xian"
                android:layout_below="@+id/ed_addreimbures_skdw"
                />
            <TextView
                android:id="@+id/daefc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/fwaedxs"
                android:layout_marginTop="@dimen/base10dp"
                android:layout_marginLeft="@dimen/base11dp"
                android:text="报销单详情"
                android:textColor="#333333"
                android:textSize="@dimen/base13dp"
                />
            <ImageView
                android:id="@+id/adaea"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/base10dp"
                android:background="@drawable/xian"
                android:layout_below="@+id/daefc"
                />
            <RelativeLayout
                android:id="@+id/argfeo"
                android:layout_below="@+id/adaea"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#f9f1fc"
                >
                <TextView
                    android:id="@+id/daeaadw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="发票类型:"
                    android:textSize="@dimen/base11dp"
                    android:textColor="#555555"
                    android:layout_marginTop="@dimen/base17dp"
                    android:layout_marginLeft="@dimen/base38dp"
                    />
                <EditText
                    android:id="@+id/ed_addreimbures_fplx"
                    android:layout_width="@dimen/base220dp"
                    android:layout_height="@dimen/base25dp"
                    android:layout_toRightOf="@+id/daeaadw"
                    style="@style/edit"
                    android:layout_marginLeft="@dimen/base5dp"
                    android:layout_marginTop="@dimen/base12dp"
                    android:hint="请选择"
                    android:editable="false"
                    />
                <ImageButton
                    android:id="@+id/image_addreimburse_fplx"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/xiala2"
                    android:layout_toRightOf="@+id/daeaadw"
                    android:layout_marginLeft="@dimen/base203dp"
                    android:layout_marginTop="@dimen/base23dp"
                    />
                <RelativeLayout
                    android:id="@+id/relative"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ed_addreimbures_fplx"
                    android:visibility="gone"
                    >
                <TextView
                    android:id="@+id/mdaedw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="金        额:"
                    android:textSize="@dimen/base11dp"
                    android:textColor="#555555"
                    android:layout_marginTop="@dimen/base17dp"
                    android:layout_marginLeft="@dimen/base38dp"
                    />
                <EditText
                    android:id="@+id/ed_addreimbures_je"
                    android:layout_width="@dimen/base220dp"
                    android:layout_height="@dimen/base25dp"
                    android:layout_toRightOf="@+id/mdaedw"
                    style="@style/edit"
                    android:layout_marginLeft="@dimen/base5dp"
                    android:layout_marginTop="@dimen/base12dp"
                    android:hint="单行输入"
                    android:digits="@string/register_name_digits"
                    />
                <TextView
                    android:id="@+id/adedqqw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="税        率:"
                    android:textSize="@dimen/base11dp"
                    android:layout_marginTop="@dimen/base17dp"
                    android:layout_marginLeft="@dimen/base38dp"
                    android:layout_below="@+id/mdaedw"

                    />
                <EditText
                    android:id="@+id/ed_addreimbures_sl"
                    android:layout_width="@dimen/base220dp"
                    android:layout_height="@dimen/base25dp"
                    android:layout_toRightOf="@+id/adedqqw"
                    style="@style/edit"
                    android:layout_marginLeft="@dimen/base5dp"
                    android:layout_marginTop="@dimen/base12dp"
                    android:layout_below="@+id/mdaedw"
                    android:hint="单行输入"
                    android:digits="@string/register_name_digits"
                    />
                <TextView
                    android:id="@+id/dadae"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="税        额:"
                    android:textSize="@dimen/base11dp"
                    android:textColor="#555555"
                    android:layout_marginTop="@dimen/base17dp"
                    android:layout_marginLeft="@dimen/base38dp"
                    android:layout_below="@+id/adedqqw"

                    />
                <EditText
                    android:id="@+id/ed_addreimbures_se"
                    android:layout_width="@dimen/base220dp"
                    android:layout_height="@dimen/base25dp"
                    android:layout_toRightOf="@+id/dadae"
                    style="@style/edit"
                    android:layout_marginLeft="@dimen/base5dp"
                    android:layout_marginTop="@dimen/base12dp"
                    android:layout_below="@+id/adedqqw"
                    android:hint="单行输入"
                    android:digits="@string/register_name_digits"
                    android:enabled="false"
                    android:textColor="#000000"
                    />
                </RelativeLayout>
                <TextView
                    android:id="@+id/dadateda"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="申请时间:"
                    android:textSize="@dimen/base11dp"
                    android:textColor="#555555"
                    android:layout_marginTop="@dimen/base17dp"
                    android:layout_marginLeft="@dimen/base38dp"
                    android:layout_below="@+id/relative"
                    />
                <EditText
                    android:id="@+id/ed_addreimbures_sqsj"
                    android:layout_width="@dimen/base220dp"
                    android:layout_height="@dimen/base25dp"
                    android:layout_toRightOf="@+id/dadateda"
                    style="@style/edit"
                    android:layout_marginLeft="@dimen/base5dp"
                    android:layout_marginTop="@dimen/base12dp"
                    android:layout_below="@+id/relative"
                    android:hint="请选择"
                    android:editable="false"
                    />
                <ImageButton
                    android:id="@+id/but_time"
                    style="@style/Ibutton"
                    android:layout_alignBottom="@+id/ed_addreimbures_sqsj"
                    android:layout_alignRight="@+id/ed_addreimbures_sqsj"
                    android:layout_alignTop="@+id/ed_addreimbures_sqsj"
                    />
                <TextView
                    android:id="@+id/aeffafw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="申请报销金额:"
                    android:textSize="@dimen/base11dp"
                    android:textColor="#555555"
                    android:layout_marginTop="@dimen/base17dp"
                    android:layout_marginLeft="@dimen/base17dp"
                    android:layout_below="@+id/dadateda"
                    />
                <EditText
                    android:id="@+id/ed_addreimbures_sqbxje"
                    android:layout_width="@dimen/base220dp"
                    android:layout_height="@dimen/base25dp"
                    android:layout_toRightOf="@+id/aeffafw"
                    style="@style/edit"
                    android:layout_marginLeft="@dimen/base5dp"
                    android:layout_marginTop="@dimen/base12dp"
                    android:layout_below="@+id/dadateda"
                    android:hint="元"
                    android:digits="@string/register_name_digits"
                    />
                <TextView
                    android:id="@+id/aefokp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="申请金额大写:"
                    android:textSize="@dimen/base11dp"
                    android:textColor="#555555"
                    android:layout_marginTop="@dimen/base17dp"
                    android:layout_marginLeft="@dimen/base17dp"
                    android:layout_below="@+id/aeffafw"
                    />
                <EditText
                    android:id="@+id/ed_addreimbures_sqjedx"
                    android:layout_width="@dimen/base220dp"
                    android:layout_height="@dimen/base25dp"
                    android:layout_toRightOf="@+id/aeffafw"
                    style="@style/edit"
                    android:layout_marginLeft="@dimen/base5dp"
                    android:layout_marginTop="@dimen/base12dp"
                    android:layout_below="@+id/aeffafw"
                    android:hint="元"
                    android:editable="false"
                    />
                <TextView
                    android:id="@+id/tv_bxsm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="报销说明:"
                    android:layout_below="@+id/ed_addreimbures_sqjedx"
                    android:textSize="@dimen/base11dp"
                    android:textColor="#555555"
                    android:layout_marginTop="@dimen/base30dp"
                    android:layout_marginLeft="@dimen/base38dp"
                    />
                <EditText
                    android:id="@+id/ed_addreimbures_bxsm"
                    android:layout_width="@dimen/base220dp"
                    android:layout_height="@dimen/base55dp"
                    android:layout_marginTop="@dimen/base10dp"
                    android:layout_marginLeft="@dimen/base5dp"
                    android:layout_toRightOf="@+id/tv_bxsm"
                    android:layout_below="@+id/ed_addreimbures_sqjedx"
                    style="@style/edit"
                    />
                <TextView
                    android:id="@+id/tv_addreimbures_sqr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/base13dp"
                    android:layout_below="@+id/ed_addreimbures_bxsm"
                    android:layout_marginTop="@dimen/base21dp"
                    android:layout_marginLeft="@dimen/base48dp"
                    />

                <ImageView
                    android:id="@+id/daeff"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_addreimbures_sqr"
                    android:layout_marginTop="@dimen/base10dp"
                    />
                <RelativeLayout
                    android:id="@+id/aefooaf"
                    android:layout_below="@+id/daeff"
                    android:background="#ffffff"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/afrfs"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="业务科目"
                         android:textSize="@dimen/base13dp"
                         android:layout_marginTop="@dimen/base10dp"
                         android:layout_marginLeft="@dimen/base11dp"
                        />
                    <TextView
                        android:id="@+id/tianjia"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableLeft="@drawable/tianjia_select"
                        android:layout_centerVertical="true"
                        android:text="添加"
                        android:textSize="@dimen/base13dp"
                        android:textColor="#12b7f5"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/base10dp"
                        />
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/xian"
                        android:layout_below="@+id/afrfs"
                        android:layout_marginTop="@dimen/base10dp"
                        />
                </RelativeLayout>
                <ListView
                    android:id="@+id/list_add_ywkm"
                    android:layout_below="@+id/aefooaf"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/base150dp">
                </ListView>
                <TextView
                    android:id="@+id/qdwqd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="小写金额:"
                    android:textSize="@dimen/base11dp"
                    android:textColor="#555555"
                    android:layout_marginTop="@dimen/base17dp"
                    android:layout_marginLeft="@dimen/base17dp"
                    android:layout_below="@+id/list_add_ywkm"
                    />
                <EditText
                    android:id="@+id/ed_addreimbures_xxje"
                    android:layout_width="@dimen/base220dp"
                    android:layout_height="@dimen/base25dp"
                    android:layout_toRightOf="@+id/aeffafw"
                    style="@style/edit"
                    android:layout_marginLeft="@dimen/base5dp"
                    android:layout_marginTop="@dimen/base12dp"
                    android:layout_below="@+id/list_add_ywkm"
                    android:hint="元"
                    android:editable="false"
                    />
                <TextView
                    android:id="@+id/awdaefaf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="大写金额:"
                    android:textSize="@dimen/base11dp"
                    android:textColor="#555555"
                    android:layout_marginTop="@dimen/base17dp"
                    android:layout_marginLeft="@dimen/base17dp"
                    android:layout_below="@+id/qdwqd"
                    />
                <EditText
                    android:id="@+id/ed_addreimbures_dxje"
                    android:layout_width="@dimen/base220dp"
                    android:layout_height="@dimen/base25dp"
                    android:editable="false"
                    android:layout_toRightOf="@+id/aeffafw"
                    style="@style/edit"
                    android:layout_marginLeft="@dimen/base5dp"
                    android:layout_marginTop="@dimen/base12dp"
                    android:layout_below="@+id/qdwqd"
                    android:hint="元"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/base40dp"
                    android:layout_below="@+id/awdaefaf"
                    />
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/argfeo"
                >
                <Button
                    android:id="@+id/but_addreimbures_fanhui"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/base50dp"
                    android:layout_weight="1"
                    android:text="返回"
                    android:textColor="#000000"
                    android:background="#ffffff"
                    android:textSize="@dimen/base18dp"
                    />
                <Button
                    android:id="@+id/but_addreimbures_save"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/base50dp"
                    android:layout_weight="1"
                    android:text="保存"
                    android:textColor="#ffffff"
                    android:background="#12b7f5"
                    android:textSize="@dimen/base18dp"
                    android:layout_toRightOf="@+id/but_addreimbures_fanhui"
                    />
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
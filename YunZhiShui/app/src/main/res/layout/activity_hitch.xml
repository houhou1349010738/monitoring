<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f2f2f2"
    >
    <include layout="@layout/toolbar_hitch" android:id="@+id/lisdysad"/>
    <RelativeLayout
        android:id="@+id/lfaeiew"
        android:layout_width="match_parent"
        android:layout_height="@dimen/base43dp"
        android:layout_below="@+id/lisdysad"
        android:background="#ffffff"
        >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:text="故障信息生成列表"
        android:textSize="@dimen/base13dp"
        android:textColor="#333333"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/base10dp"
        />
    </RelativeLayout>
    <com.smartwasser.yunzhishui.view.RefreshLayout
        android:id="@+id/swipe_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/lfaeiew"
        android:layout_marginTop="@dimen/base4dp"
        >
    <ListView
        android:id="@+id/list_hitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:divider="#f2f2f2"
        android:dividerHeight="@dimen/base7dp"
        android:background="#ffffff"
        >
    </ListView>
    </com.smartwasser.yunzhishui.view.RefreshLayout>
</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    >
     <include layout="@layout/toolbar_hitch" android:id="@+id/safasf"/>
     <TextView
         android:id="@+id/sfasfaf"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:text="车辆选择"
         android:textSize="@dimen/base15dp"
         android:textColor="#010101"
         android:layout_marginTop="@dimen/base12dp"
         android:layout_below="@+id/safasf"
         android:layout_marginLeft="@dimen/base10dp"
         />
    <ImageView
        android:id="@+id/asd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sfasfaf"
        android:background="@drawable/xian"
        android:layout_marginTop="@dimen/base12dp"
        />
    <TextView
        android:id="@+id/tv_chehao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="车号:"
        android:textSize="@dimen/base15dp"
        android:textColor="#101010"
        android:layout_marginTop="@dimen/base20dp"
        android:layout_marginLeft="@dimen/base13dp"
        android:layout_below="@+id/asd"
        />
    <EditText
        android:id="@+id/chehao"
        android:layout_width="@dimen/base200dp"
        android:layout_height="@dimen/base36dp"
        android:layout_marginLeft="@dimen/base9dp"
        android:layout_marginTop="@dimen/base13dp"
        android:layout_below="@+id/asd"
        android:layout_toRightOf="@+id/tv_chehao"
        android:hint="单行输入"
        style="@style/edit"
        />
    <Button
        android:id="@+id/but_chaxun"
        android:layout_width="@dimen/base80dp"
        android:layout_height="@dimen/base36dp"
        android:text="查询"
        android:background="@drawable/complet"
        android:textSize="@dimen/base15dp"
        android:textColor="#12b7f5"
        android:layout_marginLeft="@dimen/base8dp"
        android:layout_marginTop="@dimen/base13dp"
        android:layout_below="@+id/asd"
        android:layout_toRightOf="@+id/chehao"
        />
    <TextView
        android:id="@+id/tv_chezu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="车组:"
        android:textSize="@dimen/base15dp"
        android:textColor="#101010"
        android:layout_marginTop="@dimen/base30dp"
        android:layout_marginLeft="@dimen/base13dp"
        android:layout_below="@+id/tv_chehao"
        />
    <EditText
        android:id="@+id/chezu"
        android:layout_width="@dimen/base200dp"
        android:layout_height="@dimen/base36dp"
        android:layout_marginLeft="@dimen/base9dp"
        android:layout_marginTop="@dimen/base13dp"
        android:layout_below="@+id/chehao"
        android:layout_toRightOf="@+id/tv_chezu"
        android:hint="单行输入"
        style="@style/edit"
        android:editable="false"
        />
    <ImageButton
        android:id="@+id/but_chezu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/chehao"
        android:layout_toRightOf="@+id/tv_chezu"
        android:background="@drawable/xiala2"
        android:layout_marginTop="@dimen/base30dp"
        android:layout_marginLeft="@dimen/base190dp"
        />
    <GridView
        android:id="@+id/list_car"
        android:layout_width="match_parent"
        android:layout_height="@dimen/base180dp"
        android:background="#ebebeb"
        android:layout_below="@+id/chezu"
        android:numColumns="3"
        android:layout_marginTop="@dimen/base10dp"
        >
    </GridView>
    <TextView
        android:id="@+id/asda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="时间选择"
        android:textColor="#010101"
        android:textSize="@dimen/base15dp"
        android:layout_below="@+id/list_car"
        android:layout_marginTop="@dimen/base10dp"
        android:layout_marginLeft="@dimen/base12dp"
        />
    <ImageView
        android:id="@+id/erw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/asda"
        android:background="@drawable/xian"
        android:layout_marginTop="@dimen/base10dp"
        />
    <TextView
        android:id="@+id/fasf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="开始时间:"
        android:layout_below="@+id/erw"
        android:layout_marginLeft="@dimen/base14dp"
        android:layout_marginTop="@dimen/base13dp"
        android:textColor="#010101"
        android:textSize="@dimen/base13dp"
        />
        <EditText
            android:id="@+id/ed_start_dates"
            style="@style/edit"
            android:layout_width="@dimen/base200dp"
            android:layout_marginLeft="@dimen/base13dp"
            android:layout_marginTop="@dimen/base8dp"
            android:height="@dimen/base25dp"
            android:width="@dimen/base214dp"
            android:layout_toRightOf="@id/fasf"
            android:layout_below="@+id/erw"
            android:editable="false"
            />
        <ImageButton
            android:id="@+id/but_car_starttime"
            style="@style/Ibutton"
            android:layout_alignBottom="@+id/ed_start_dates"
            android:layout_alignRight="@+id/ed_start_dates"
            android:layout_alignTop="@+id/ed_start_dates"
            />
    <TextView
        android:id="@+id/jsshij"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="结束时间:"
        android:layout_below="@+id/fasf"
        android:layout_marginLeft="@dimen/base14dp"
        android:layout_marginTop="@dimen/base13dp"
        android:textColor="#010101"
        android:textSize="@dimen/base13dp"
        />
    <EditText
        android:id="@+id/ed_end_dates"
        style="@style/edit"
        android:layout_width="@dimen/base200dp"
        android:layout_marginLeft="@dimen/base13dp"
        android:layout_marginTop="@dimen/base8dp"
        android:height="@dimen/base25dp"
        android:width="@dimen/base214dp"
        android:layout_toRightOf="@id/jsshij"
        android:layout_below="@id/fasf"
        android:editable="false"
        />
    <ImageButton
        android:id="@+id/but_car_endtime"
        style="@style/Ibutton"
        android:layout_alignBottom="@+id/ed_end_dates"
        android:layout_alignRight="@+id/ed_end_dates"
        android:layout_alignTop="@+id/ed_end_dates"
        />
    <TextView
        android:id="@+id/kjfshi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="快捷选择:"
        android:layout_below="@+id/jsshij"
        android:layout_marginLeft="@dimen/base14dp"
        android:layout_marginTop="@dimen/base13dp"
        android:textColor="#010101"
        android:textSize="@dimen/base13dp"
        />
    <EditText
        android:id="@+id/ed_kaijie_dates"
        style="@style/edit"
        android:layout_width="@dimen/base200dp"
        android:layout_marginLeft="@dimen/base13dp"
        android:layout_marginTop="@dimen/base8dp"
        android:height="@dimen/base25dp"
        android:width="@dimen/base214dp"
        android:layout_toRightOf="@id/kjfshi"
        android:layout_below="@id/jsshij"
        android:editable="false"
        />
    <ImageButton
        android:id="@+id/but_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/xiala2"
        android:layout_toRightOf="@id/kjfshi"
        android:layout_below="@id/jsshij"
        android:layout_marginLeft="@dimen/base195dp"
        android:layout_marginTop="@dimen/base17dp"
        />
    <LinearLayout
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="@dimen/base46dp">
    <Button
        android:id="@+id/queren"
        android:layout_width="0dp"
        android:layout_height="@dimen/base46dp"
        android:layout_weight="1"
        android:background="@drawable/login_select_pressed"
        android:text="确定"
        android:textColor="#ffffff"
        android:textSize="@dimen/base16dp"
        />
        <Button
            android:id="@+id/quxiao"
            android:layout_width="0dp"
            android:layout_height="@dimen/base46dp"
            android:layout_weight="1"
            android:text="取消"
            android:textColor="#010101"
            android:background="@drawable/commit_select"
            android:textSize="@dimen/base16dp"
            />
    </LinearLayout>
</RelativeLayout>
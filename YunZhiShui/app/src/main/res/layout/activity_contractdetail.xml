<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#f2f2f2"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/toolbar" android:id="@+id/dadwda"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/base40dp"
        android:layout_below="@+id/dadwda"
        android:background="#ffffff"
        android:id="@+id/liasdwu"

        >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="基本信息"
        android:layout_marginTop="@dimen/base12dp"
        android:layout_marginLeft="@dimen/base11dp"
        android:textSize="@dimen/base15dp"

        android:textColor="#333333"
        />
    <TextView
        android:id="@+id/tv_contract_htbh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/base13dp"
        android:textColor="#5091d3"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/base11dp"
        android:layout_marginTop="@dimen/base12dp"
        />
    </RelativeLayout>
    <ScrollView
        android:layout_below="@+id/liasdwu"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/liasdwu"
            android:background="#ffffff"
            android:layout_marginTop="@dimen/base7dp"
            >
    <ImageView
        android:id="@+id/waedadawd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base15dp"
        android:layout_marginLeft="@dimen/base13dp"
        android:background="@drawable/qian"
        />
    <TextView
        android:id="@+id/adesad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="合同金额"
        android:textSize="@dimen/base13dp"
        android:textColor="#929292"
        android:layout_toRightOf="@+id/waedadawd"
        android:layout_marginLeft="@dimen/base7dp"
        android:layout_marginTop="@dimen/base15dp"
        android:layout_below="@+id/adwwdwa"
        />
    <ImageView
        android:id="@+id/waedada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base15dp"
        android:layout_marginLeft="@dimen/base109dp"
        android:layout_below="@+id/adwwdwa"
        android:background="@drawable/qian2"
        android:layout_toRightOf="@+id/adesad"
        />
    <TextView
        android:id="@+id/adesads"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="未收金额"
        android:textSize="@dimen/base13dp"
        android:textColor="#929292"
        android:layout_toRightOf="@+id/waedada"
        android:layout_marginLeft="@dimen/base7dp"
        android:layout_marginTop="@dimen/base15dp"
        android:layout_below="@+id/adwwdwa"
        />
    <TextView
        android:id="@+id/tv_contract_htje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/waedada"
        android:layout_marginLeft="@dimen/base1dp"
        android:layout_marginTop="@dimen/base15dp"
        android:textColor="#333333"
        android:textSize="@dimen/base20dp"
        />
    <TextView
        android:id="@+id/tv_contract_wsje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/waedada"
        android:layout_marginTop="@dimen/base15dp"
        android:textColor="#fc3b3c"
        android:textSize="@dimen/base20dp"
        android:layout_toRightOf="@+id/waedada"
        />
    <ImageView
        android:id="@+id/adqwd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_contract_wsje"
        android:layout_marginTop="@dimen/base15dp"
        android:background="@drawable/xian"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="原始合同金额"
        android:layout_marginTop="@dimen/base12dp"
        android:layout_marginLeft="@dimen/base13dp"
        android:textSize="@dimen/base13dp"
        android:textColor="#929292"
        android:layout_below="@+id/adqwd"
        />
    <TextView
        android:id="@+id/tv_contract_htysje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base12dp"
        android:textSize="@dimen/base13dp"
        android:textColor="#333333"
        android:layout_below="@+id/adqwd"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/base20dp"
        />
    <TextView
        android:id="@+id/zhankai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="收起"
        android:layout_below="@+id/tv_contract_htysje"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/base15dp"
        android:textColor="#ff8200"
        android:textSize="@dimen/base13dp"
        android:layout_marginTop="@dimen/base23dp"
        android:drawableRight="@drawable/zhan1"
        />
            <ImageView
                android:id="@+id/adwdaaww"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/zhankai"
                android:layout_marginTop="@dimen/base10dp"
                android:background="@drawable/xian"
                />
            <RelativeLayout
                android:id="@+id/rayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/adwdaaww"
                android:background="#f9f1fc"
                >
                <TextView
                    android:id="@+id/tv_contract_htjsfs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/base11dp"
                    android:textSize="@dimen/base13dp"
                    android:textColor="#333333"
                    android:layout_marginLeft="@dimen/base11dp"
                    />
                <ImageView
                    android:id="@+id/awfdsas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_contract_htjsfs"
                    android:background="@drawable/xian"
                    android:layout_marginTop="@dimen/base11dp"
                    />
                <TextView
                    android:id="@+id/tv_contract_htmc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/awfdsas"
                    android:layout_marginTop="@dimen/base14dp"

                    android:layout_marginLeft="@dimen/base11dp"
                    android:textSize="@dimen/base13dp"
                    android:textColor="#555555"
                    />
                <TextView
                    android:id="@+id/tv_contract_xmmc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_contract_htmc"
                    android:layout_marginTop="@dimen/base11dp"

                    android:layout_marginLeft="@dimen/base11dp"
                    android:textSize="@dimen/base13dp"
                    android:textColor="#555555"
                    />
                <TextView
                    android:id="@+id/tv_contract_htlx"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_contract_htmc"
                    android:layout_marginTop="@dimen/base11dp"

                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/base14dp"
                    android:textSize="@dimen/base13dp"
                    android:textColor="#555555"
                    />
                <TextView
                    android:id="@+id/tv_contract_jf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_contract_xmmc"
                    android:layout_marginTop="@dimen/base11dp"

                    android:layout_marginLeft="@dimen/base11dp"
                    android:textSize="@dimen/base13dp"
                    android:textColor="#555555"
                    />
                <TextView
                    android:id="@+id/tv_contract_yf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_contract_jf"
                    android:layout_marginTop="@dimen/base11dp"
                    android:layout_marginLeft="@dimen/base11dp"
                    android:textSize="@dimen/base13dp"
                    android:textColor="#555555"
                    />
                <TextView
                    android:id="@+id/tv_contract_qdsj"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_contract_yf"
                    android:layout_marginTop="@dimen/base11dp"
                    android:layout_marginLeft="@dimen/base11dp"
                    android:textSize="@dimen/base13dp"
                    android:textColor="#555555"
                    />
                <ImageView
                    android:id="@+id/adedww"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_contract_qdsj"
                    android:layout_marginTop="@dimen/base12dp"
                    android:background="@drawable/xian"
                    />
            </RelativeLayout>
            <TextView
                android:id="@+id/falsriorio"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="数据列表"
                android:layout_marginTop="@dimen/base10dp"
                android:layout_marginLeft="@dimen/base11dp"
                android:textSize="@dimen/base15dp"
                android:textColor="#333333"
                android:layout_below="@+id/rayout"
                />
      <TextView
         android:id="@+id/tianjia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/tianjia_select"
        android:layout_centerVertical="true"
        android:text="添加"
        android:layout_below="@+id/rayout"
        android:layout_marginTop="@dimen/base12dp"
        android:textSize="@dimen/base13dp"
        android:textColor="#12b7f5"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/base15dp"
        />
    <ImageView
        android:id="@+id/adwada"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/falsriorio"
        android:background="@drawable/xian"
        android:layout_marginTop="@dimen/base12dp"
        />
     <ListView
        android:id="@+id/list_contract_detail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/adwada"
        >
     </ListView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/base15dp"
                android:layout_below="@+id/list_contract_detail"
                />
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
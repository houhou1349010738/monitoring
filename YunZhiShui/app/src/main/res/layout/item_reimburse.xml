<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
    >
    <TextView
        android:id="@+id/wew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/base11dp"
        android:layout_marginTop="@dimen/base13dp"
        android:text="报销单类别:"
        android:textSize="@dimen/base15dp"
        android:textColor="#333333"
        />
    <TextView
        android:id="@+id/tv_reimburse_bxdlb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/wew"
        android:layout_marginLeft="@dimen/base4dp"
        android:layout_marginTop="@dimen/base13dp"
        android:text="现金支出报销单"
        android:textColor="#12b7f5"
        android:textSize="@dimen/base15dp"
        />
    <TextView
        android:id="@+id/tv_reimburse_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/base15dp"
        android:layout_marginTop="@dimen/base13dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/base13dp"
        />
    <TextView
        android:id="@+id/sad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base21dp"
        android:layout_below="@+id/wew"
        android:text="单位名称:"
        android:textSize="@dimen/base13dp"
        android:textColor="#555555"
        android:layout_marginLeft="@dimen/base11dp"
        />
    <TextView
        android:id="@+id/tv_reimburse_je"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base21dp"
        android:layout_below="@+id/wew"
        android:text="金额:213124"
        android:textSize="@dimen/base13dp"
        android:textColor="#555555"
        android:layout_marginRight="@dimen/base13dp"
        android:layout_alignParentRight="true"
        />
    <TextView
        android:id="@+id/tv_reimburse_xmmc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="图书馆项目"
        android:textSize="@dimen/base13dp"
        android:textColor="#333333"
        android:layout_toRightOf="@+id/sad"
        android:layout_marginTop="@dimen/base21dp"
        android:layout_below="@+id/wew"
        android:layout_marginLeft="@dimen/base4dp"
        />
    <TextView
        android:id="@+id/rwrew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base11dp"
        android:layout_below="@+id/sad"
        android:text="业务类型:"
        android:textSize="@dimen/base13dp"
        android:textColor="#555555"
        android:layout_marginLeft="@dimen/base11dp"
        />
    <TextView
        android:id="@+id/tv_reimburse_rqr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base11dp"
        android:layout_below="@+id/sad"
        android:text="申请人:萌萌"
        android:textSize="@dimen/base13dp"
        android:textColor="#555555"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/base13dp"
        />
    <TextView
        android:id="@+id/tv_reimburse_ywlx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="财务费用"
        android:textSize="@dimen/base13dp"
        android:textColor="#333333"
        android:layout_toRightOf="@+id/rwrew"
        android:layout_marginTop="@dimen/base11dp"
        android:layout_below="@+id/sad"
        android:layout_marginLeft="@dimen/base4dp"
        />
    <TextView
        android:id="@+id/wrqww"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/base11dp"
        android:layout_below="@+id/rwrew"
        android:text="申请日期:"
        android:textSize="@dimen/base13dp"
        android:textColor="#555555"
        android:layout_marginLeft="@dimen/base11dp"
        />
    <TextView
        android:id="@+id/tv_reimburse_sqrq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2016-09-08"
        android:textSize="@dimen/base13dp"
        android:textColor="#333333"
        android:layout_toRightOf="@+id/wrqww"
        android:layout_marginTop="@dimen/base11dp"
        android:layout_below="@+id/rwrew"
        android:layout_marginLeft="@dimen/base4dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_reimburse_sqrq"
        android:layout_marginTop="@dimen/base10dp"
        />
</RelativeLayout>